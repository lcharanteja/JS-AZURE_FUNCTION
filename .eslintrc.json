{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {},
  "overrides": [
    {
      "files": ["**/*.test.js", "**/*.test.jsx", "**/testing/**"],
      "env": {
        "jest": true
      }
    }
  ]
}
